. ./defs

set -e

cat >expected <<EOF
transitions: 16
edges: 8
states: 4
nondeterministic states: 0
EOF

# These two equivalent formulae used to produce
# minimized automata of different sizes...
run 0 ../ikwiad -Rm -kt 'a | X(Gd|Fa)' > out
run 0 ../ikwiad -Rm -kt 'Fa | XGd' > out2

cmp out expected
cmp out2 expected

cat >input <<EOF
HOA: v1
name: "XXFp1"
States: 5
Start: 4
AP: 1 "p1"
acc-name: Buchi
Acceptance: 1 Inf(0)
properties: implicit-labels state-acc complete deterministic
--BODY--
State: 0 {0}
0 0
State: 1 {0}
3 0
State: 2
1 1
State: 3
3 0
State: 4
2 2
--END--
EOF

cat >expected <<EOF
HOA: v1
States: 4
Start: 2
AP: 1 "p1"
acc-name: Buchi
Acceptance: 1 Inf(0)
properties: implicit-labels state-acc complete deterministic terminal
--BODY--
State: 0 {0}
0 0
State: 1
3 3
State: 2
1 1
State: 3
3 0
--END--
EOF

autfilt --small --high -C -Hi input > output
diff output expected
